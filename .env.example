# Discord Bot Token from Discord Developer Portal
DISCORD_BOT_TOKEN=YOUR_DISCORD_BOT_TOKEN_HERE

# Google Gemini API Key from Google AI Studio or Google Cloud Console
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# ID of the Discord channel where the bot will primarily operate
# Right-click on the channel in Discord, then "Copy ID". Developer Mode must be enabled in Discord settings.
DESIGNATED_CHANNEL_ID=YOUR_DISCORD_CHANNEL_ID_HERE

# Optional: Set a log level (e.g., DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Gemini Model Name (e.g., gemini-pro, gemini-1.5-flash)
GEMINI_MODEL_NAME=gemini-1.5-flash
